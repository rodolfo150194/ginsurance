{
  "version": 2,
  "builds": [
    {
      "src": "gi_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }

  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "gi_app/wsgi.py"
    }
  ]
}
